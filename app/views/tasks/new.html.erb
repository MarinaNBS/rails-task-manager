<%= form_with model: @task do |f|%>
  <%= f.label :title%>
  <%= f.text_field :title%>

  <%= f.label :details%>
  <%= f.text_field :details%>

  <% f.label :completed%>

  <%= f.submit value:"Create Task" %>
<% end %>
 <hr>
  <%= link_to tasks_path do %>
    <i class="fa-solid fa-arrow-rotate-left"></i> Back to list
  <% end %>
